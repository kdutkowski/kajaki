graph example_1 {
  layout=dot;
  node [shape=record];

    a [label="1|2"];
    b [label="2|3"];
    c [label="3|5"];
    d [label="5|4"];
    e [label="5|6"];
    f [label="2|6"];
    g [label="1|6"];


    e -- a;
    a -- c [color=red];
    b -- e;
    b -- g [color=red];
    b -- d;
    c -- f;
    d -- f [color=red];
    d -- g;

    { rank=same; a b c d}
    { rank=same; e f g}
}
